{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon index.js",
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build",
    "client": "npm start --prefix client",
    "build": "npm install && (npm run build --prefix client)",
    "dev": "concurrently \"nodemon --inspect index.js\" \"cd client && npm start\""
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {}
}
